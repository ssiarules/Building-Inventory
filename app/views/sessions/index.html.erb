<h1>Welcome to Building Management Inventory System!</h1>

<%= link_to "Log In", "/login" %> <br> <br>


<%= link_to "Sign Up", "/signup" %><br> <br>


<% if session[:user_id] %>
  <h1><%= @user.username %></h1>
  <h2>Email: <%= @user.email %></h2>
  <h2>Github UID: <%= @user.uid %></h2>
  <img src="<%= @user.image %>">
<% else %>
  <%= link_to('Github Login!', '/auth/github') %>
<% end %>

